fixed4 frag (v2f i) : SV_Target
            {
                sampler2D mainTexCopy = _MainTex;
                
              
                fixed4 col = tex2D(_MainTex, i.uv);
                
                fixed4 col2 = tex2D(_MainTex, i.uv + _UvOffset);
                // just invert the colors
                //col = 1 - col;
                return (col + col2)/2;
            }
            
